<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">


    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="inertia_ssr">
                <display-name xml:lang="x-default">Inertia SSR Enabled?</display-name>
                <description xml:lang="x-default">Enable Inertia SSR Server</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="storefront_urls">
                <display-name xml:lang="x-default">Enable Storefront URLs?</display-name>
                <description xml:lang="x-default">Should be sync with Site Enable Storefront URLs config</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
        </custom-attribute-definitions>
         <group-definitions>
            <attribute-group group-id="Inertia">
                <display-name xml:lang="x-default">Inertia: Configuration Properties</display-name>
                <attribute attribute-id="inertia_ssr"/>
                <attribute attribute-id="storefront_urls"/>
            </attribute-group>
        </group-definitions>
    </type-extension>


</metadata>